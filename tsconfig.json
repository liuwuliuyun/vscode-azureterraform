{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "target": "ES2022",
        "outDir": "out",
        "lib": [
            "ES2022"
        ],
        "sourceMap": true,
        "rootDir": "src",
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noUnusedParameters": true
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}